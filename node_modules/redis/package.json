    "nyc": "^3.2.2",
    "optional-dev-dependency": "^1.1.0",
    "tcp-port-used": "^0.1.2",
    "uuid": "^2.0.1",
    "win-spawn": "^2.0.0",
    "bluebird": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/NodeRedis/node_redis.git"
  },
  "bugs": {
